<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/daluzy_title_page04"/>

    <EditText
        android:id="@+id/getFeedBack"
        android:layout_width="320dp"
        android:layout_marginTop="50dp"
        android:padding="5dp"
        android:layout_height="400dp"
        android:background="@drawable/bg_et_2"
        android:layout_gravity="center"
        android:singleLine="false"
        android:hint="@string/yijian_tixing"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="left"
        android:textStyle="bold"/>

    <EditText
        android:id="@+id/getTel"
        android:layout_width="320dp"
        android:layout_margin="15dp"
        android:padding="5dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_et_2"
        android:layout_gravity="center"
        android:singleLine="false"
        android:hint="请留下您的联系方式："
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="left"
        android:textStyle="bold" />

    <Button
        android:id="@+id/postFeedback"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@drawable/bg_btn4"
        android:layout_gravity="center"
        android:text="提交"
        android:textColor="#ffffff"
        android:textSize="25sp"
        android:layout_marginTop="50dp"/>




</LinearLayout>